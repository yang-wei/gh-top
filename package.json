{
  "name": "gh-star",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/yang-wei/gh-top"
  },
  "keywords": [
    "github",
    "star",
    "treemap"
  ],
  "scripts": {
    "test": "npm run test-server & npm run test-comp",
    "test-server": "./node_modules/.bin/mocha --reporter list server/test",
    "test-comp": "./node_modules/.bin/mocha -R spec --compilers jsx:client/components/tests/helper.js client/components/tests/*test.js",
    "build": "browserify -t reactify client/client.js -o public/bundle.js",
    "server:dev": "nodemon server/server.js",
    "server": "supervisor server/server.js",
    "start:dev": "export NODE_ENV=development && npm run build && npm run server:dev",
    "start": "export NODE_ENV=production && npm run build && npm run server"
  },
  "main": "server.js",
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 1.4"
  },
  "author": "yang-wei",
  "license": "MIT",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jsdom": "^1.4.1",
    "mocha": "^2.0.1"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.9.2",
    "browserify": "^7.0.3",
    "d3": "^3.4.13",
    "express": "^4.10.2",
    "fibers": "^1.0.2",
    "mongoskin": "^1.4.4",
    "node-jsx": "^0.12.4",
    "react": "^0.12.1",
    "react-async": "^2.0.1",
    "reactify": "^0.17.1",
    "request": "^2.51.0",
    "superagent": "^0.21.0",
    "supervisor": "^0.6.0"
  }
}
